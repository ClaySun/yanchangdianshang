<script>
$(document).ready(function () {
	$("#submit").click(function(){
		var jsonArray = [];
		var flag = true;
		$("input[name='ckj']").each(function(){
			var obj = {};
			obj.htxh = $(this).attr("htxh");
			obj.ckj = $(this).val();
			if (!checkNumberFormat(obj.ckj+"", "参考价", true, 2, 0, 10000000, false, true)) {
				return flag = false;
			}
			jsonArray.push(obj);
		});
		
		if(!flag){
			return false;
		}
		
		var jsonStr = JSON.stringify(jsonArray);
		if(confirm("确认提交吗？")) {
			$("#jsonStr").val(jsonStr);
			$(this).attr('disabled',"true");
			$("#gform").attr("action","/arplatform/exp/contract/sell/manage/setCkj.do");
			$("#gform").submit();
		}
	});
});
</script>


