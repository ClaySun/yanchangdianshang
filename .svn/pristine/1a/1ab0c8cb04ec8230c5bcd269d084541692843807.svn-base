			$(this).find(".list-details").show()
		}).mouseleave(function() {
			$(this).find(".list-details").hide()
		})
		//登录和注册按钮背景变化
		$(".login-btn").mouseenter(function() {
			$(this).find(".icon-denglu").removeClass("color-fd7636").addClass("color-fff")
