	<script type="text/javascript">
		//导航详细菜单的显示和隐藏
		$(".sm-list-details").hide()
		$(".sm-banner-on-left-list>li").mouseenter(function() {
			$(this).find(".sm-list-details").show()
		}).mouseleave(function() {
			$(this).find(".sm-list-details").hide()
		})
		//登录和注册按钮背景变化
		$(".login-btn").mouseenter(function() {
			$(this).find(".icon-denglu").removeClass("color-fd7636").addClass("color-fff")
		}).mouseleave(function() {
			$(this).find(".icon-denglu").removeClass("color-fff").addClass("color-fd7636")
		})
		$(".register-btn").mouseenter(function() {
			$(this).find(".icon-zhuce").removeClass("color-fd7636").addClass("color-fff")
		}).mouseleave(function() {
			$(this).find(".icon-zhuce").removeClass("color-fff").addClass("color-fd7636")
		})
		//招标公告和招标结果按钮切换
		$(".inner-title").click(function() {
			$(this).addClass("inner-title-acitve").siblings().removeClass("inner-title-acitve")
		})
		$(".wide-inner-title").click(function() {
			$(this).addClass("inner-title-acitve").siblings().removeClass("inner-title-acitve")
		})
		//电梯导航
		$(".elevator-list>li").mouseenter(function() {
			$(this).find(".second").css("background-color","#fd7636")
			$(this).find(".first").animate({right: "36px"});
		}).mouseleave(function() {
			$(".elevator-list>li").find(".first").stop(true);
			$(this).find(".second").css("background-color","#7a6e6e")
			$(this).find(".first").animate({right: "-36px"},200).stop(false,true);
		})
	</script>
