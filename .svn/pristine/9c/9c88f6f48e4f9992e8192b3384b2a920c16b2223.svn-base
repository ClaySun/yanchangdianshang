/**tabs**/
.tabPanel{ 
	/*margin-top: 10px;*/
}
.tabPanel ul{height:30px;border-bottom:1px solid #aaa;}
.tabPanel ul li{
	float:left; border:1px solid #efefef;font-size:15px;height:29px;line-height:30px;width:72px;text-align:center;cursor:pointer;
	
	border-radius:4px 4px 0 0;
	box-shadow:inset 0 1px 0 rgba(255, 255, 255, 0.5);
	background:#ddd;
	background:-moz-linear-gradient(top, #eee, #ddd);
	background:-webkit-gradient(linear, left top, left bottom, from(#eee), to(#ddd));
}
.tabPanel .hit{
	border-bottom:1px solid #fff;cursor:pointer;color:#FFF;
	background:#F00;
