/*banner*/
.sm-banner {
	width: 1200px;
	margin-left: auto;
	margin-right: auto;
	height: 400px;
	background-color: #f2f2f2;
	/*overflow: hidden;*/
}
.sm-banner-on {
	width: 1200px; margin:0 auto;
	
}
.sm-banner-on-left {
	float: left;
	background-color: rgba(255,255,255,.9);
	/*border: 1px solid #ddd;*/
	width: 200px;
	position:absolute;
	z-index:9999;
}
.sm-banner-on-left-list { position: relative;}
.sm-banner-on-left-list>li {
	/*margin-left: 12px;
	margin-right: 12px;*/
	padding-left: 12px;
	padding-right: 12px;
	height: 99px;
	line-height: 99px;
	border-bottom: 1px solid #ddd;
}
.sm-banner-on-left-list>li:hover {
	background-color: #fd7636;
}
.sm-banner-on-left-list>li:hover>.banner-on-left-list-each>a>span {
	color: #fff;
}
.sm-banner-on-left-list-each {
	/*position: relative;*/
}
.sm-list-details {
	position: absolute;
	top: 0;
	left: 200px;
	width: 600px;
	height:384px;
	background-color: rgba(255,255,255,1);
	padding: 8px 4px;
	 z-index:999;
	 opacity:0.70;filter:alpha(opacity=70); 
	
}
.list-details-title {
	display: block;
	font-weight: 500;
	height: 28px;
	line-height: 28px;
}
.list-details-details>li {
	float: left;
	margin-left: 8px;
	margin-right: 8px;
	height: 28px;
	line-height: 28px;
}
.list-details-details>li>a {
	color: #999;
}
.list-details-details>li>a:hover {
	color: #;
}
.sm-banner-on-left-list-each>.list-each {
	color: #111;
	font-size: 14px;
}
.sm-banner-on-left-list-each>a {
	color: #999;
	font-size: 12px;
	margin-right: 4px;
}
/*.banner-on-left-list-each>a:hover span {
	color: #fff;
}*/
.sm-banner-on-right {
	float: right;
	border-bottom:1px solid #efefef;
	border-top:1px solid #efefef;
	border-right:1px solid #efefef;
	width: 299px;
	height: 400px;
}
.login-area {
	/*border-bottom: 1px solid #f00;*/
}
.img-head-area {
	width: 200px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 12px;
}
.img-head {
	margin-left: 20px;
	height: 40px;
	width: 40px;
	border-radius: 50%;
	overflow: hidden;
}
.button-area {
	margin-top: 8px;
	border-bottom: 1px solid #ddd;
	border-top: 1px solid #ddd;
}
.login-btn,
.register-btn {
	width: 50%;
	text-align: center;
	padding-bottom: 8px;
	padding-top: 8px;
	cursor: pointer;
}
.login-btn:hover,
.register-btn:hover {
	background-color: #fd7636;
	color: #fff;
}
.sm-notice-area,
.news-area {
	width: 240px;
	margin-left: auto;
	margin-right: auto;
	border-bottom: 1px dashed #fd7636;
	padding-bottom: 8px;
}
.tips-area {
	width: 240px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 4px;
}
.notice-title,
.news-title {
	margin-top: 4px;
	height: 32px;
	line-height: 32px;
	width: 80px;
	color: #fd7636;
	text-align: center;
	border-bottom: 1px solid #fd7636;
}
.notice-title-blank,
.news-title-blank {
	margin-top: 4px;
	height: 32px;
	line-height: 32px;
	width: 160px;
	text-align: right;
	border-bottom: 1px solid #ddd;
}
.notice-list,
.news-list,
.tips-list {
	margin-top: 4px;
}
.notice-list>li,
.news-list>li,
.tips-list>li {
	padding-top: 2px;
	padding-bottom: 2px;
	font-size: 12px;
	/*word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;*/
}
.notice-list>li>a,
.news-list>li>a {
	font-size: 12px;
}
.news-list>li {
	list-style-type: disc;
	color: #fd7636;
	margin-left: 16px;
}
.tips-list>li {
	background-image: url(../images/trangle.png);
	background-repeat: no-repeat;
	background-position: 10px 6px;
	text-align: center;
	margin-left: 8px;
}


