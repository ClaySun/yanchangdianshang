.screen_con.shop_screen_con dl {
	width: 700px;
}

.screen_con {
	width: 100%;
}

.screen_con li {
	padding: 12px 65px 0px 85px;
	position: relative;
}

.screen_con .select-list {
	border-bottom: 1px dashed #dbdbdb;
}

.screen_con dl {
	height: 34px;
	overflow: hidden;
	float: left;
	width: 980px;
}

.screen_con span.select_title {
	width: 70px;
	padding-left: 15px;
	float: left;
	font-size: 14px;
	height: 22px;
	line-height: 22px;
	position: absolute;
	left: 0px;
	top: 12px;
}

.screen_con dd,
span.select-all {
	display: block;
	float: left;
	height: 22px;
	line-height: 22px;
	padding-bottom: 12px;
	overflow: hidden;
	padding-right: 20px;
}

span.select-all {
	width: 60px;
	padding-right: 0px;
}

.screen_con a,
span.select-all a {
	display: inline-block;
	white-space: nowrap;
	height: 22px;
	line-height: 22px;
	padding: 0 8px;
	text-decoration: none;
	color: #666;
	border-radius: 2px;
}

.screen_con .selected a,
.screen_con dd a:hover {
	color: #fff;
	background-color: #fd7636;
}

.select-no {
	color: #999
}

.screen_con .select-result a {
	padding-right: 20px;
	background: #fd7636 url(../images/little_close.png) no-repeat right center;
	color: #FFF;
}

.screen_con a.more_box,
.screen_con a.more_icon {
	background: url(../images/add.jpg) no-repeat right center;
	width: 32px;
	text-align: right;
	position: absolute;
	top: 12px;
	right: 15px;
	padding-right: 18px;
	cursor: pointer;
}

.screen_con a.more_icon {
	background: url(../images/-.jpg) no-repeat right center;
}

.screen_con dl.auto_height {
	height: auto;
}


