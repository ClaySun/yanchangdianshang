/*自定义标签*/
/*elevator电梯导航*/
.elevator {
	position: fixed;
	z-index: 999;
	right: 0;
	bottom: 20px;
}
.elevator-list>li {
	position: relative;
	width: 36px;
	height: 36px;
	text-align: center;
	cursor: pointer;
	border-bottom: 1px solid rgba(255,255,255,1);
	border-top-left-radius: 4px;
	border-bottom-left-radius: 4px;
	background-color: #7a6e6e;
}
.elevator-list>li>a {
	font-size: 12px;
	word-wrap: break-all;
	vertical-align: middle;
}
.elevator-list>li>.first {
	position: absolute; 
	top: 0; 
	right: -36px; 
	display: block; 
	height: 36px; 
	line-height: 36px;
	width: 72px; 
	color: #fff;
	border-top-left-radius: 4px;
	border-bottom-left-radius: 4px;
	background-color: #fd7636;
	z-index: 2;
}
.elevator-list>li>.second {
	position: relative;
	display: block; 
	height: 36px; 
	line-height: 36px;
	width: 36px; 
	color: #fff;
	border-top-left-radius: 4px;
	border-bottom-left-radius: 4px;
	background-color: #7a6e6e;
	z-index: 3;
}
.elevator-list>li>.second:hover {
	background: #fd7636;
}
.elevator-list>li:hover {
	background-color: #fd7636;
	border-top-left-radius: 0;
	border-bottom-left-radius: 0;
}

.elevator-list>li:hover>a { color: #fff;}
.elevator-list>li:hover>a>span { color: #fff;}
.elevator-list>.border-none { border: none;}


