html,
body,
dl,
dt,
dd,
table,
ul,
ol,
li,
select,
option,
button,
input[text],
textarea { 
	margin: 0; 
	padding: 0;
}

ol,
li { list-style: none;}
a {
	color: #333; 
	text-decoration: none;
}
a:hover {
	color: #f00; 
	text-decoration: none;
}
img { 
	border: none;
	display: block;
}
button,
input[text],
select,
textarea {
	outline: 0 none; 
	vertical-align: middle;
}
table { 
	border: none;
}
button { 
	cursor: pointer;
	border: none;
}

