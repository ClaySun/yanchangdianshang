/*工业品市场*/

.filter {
    position: relative;
    background: #fff;
    margin: 0 0 60px;
}
.filter .list {
    box-shadow: 0 1px 3px #d9dde4;
    position: relative;
    z-index: 10;
}
.filter dl {
    overflow: hidden;
    border-bottom: 1px solid #d9dde4;
    padding: 15px 0 0;
}
.filter dl dd {
    float: left;
    width: 1000px;
}
.filter dl dt {
    float: left;
    width: 100px;
    text-align: right;
    color: #474e5d;
    line-height: 32px;
    margin: 0 10px 15px 0;
}
.filter dl dd a {
    float: left;
    height: 30px;
    line-height: 30px;
    padding: 1px 19px;
    margin: 0 10px 15px 0;
}
.filter dl dd a.on {
    border: 1px solid #35aaf2;
    color: #35aaf2;
    padding: 0 18px;
    border-radius: 3px;
}
.paixu_all {
	width: 100%;
