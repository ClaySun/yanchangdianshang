/*挂牌列表重写*/
.table_list_div{
	width: 1198px;
	margin-top: 20px;
}
.table_ul{
	width: 1198px;
	overflow: hidden;
	border:1px solid #ccc;
	border-top: none;
	border-left: none;
	border-right: none;
}
.table_ul li{
	float: left;
	font-size: 14px;
	padding: 10px 0px 10px 5px;
}
.table-li{
	width: 1098px;
}
.table-li li{
	height: 66px;
	overflow: hidden;
}
.table-li li em{
	vertical-align:middle;
	display: table-cell;
	height: 66px;
	overflow: hidden;
	word-break: break-all;
}

.control-sapn{
	display: block;
	padding: 10px 0px 10px 0px;
	font-weight: bold;
}
.xuangou_a {
	display: block;
	border: 1px solid #EB544B;
	border-radius: 4px;
	padding: 4px 10px 4px 10px;
	text-align: center;
	color: #EB544B;
	width: 70px;
	margin: 30px 8px 0px 0px;
}
.table-head li{
	font-weight: bold;
}
