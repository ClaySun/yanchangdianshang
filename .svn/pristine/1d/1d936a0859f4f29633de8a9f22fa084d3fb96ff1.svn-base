				//点击华东地区全选与全不选
				dqclick("hd",".hdclass");
				//点击中南地区全选与全不选
				dqclick("zn",".znclass");
				//点击西南地区全选与全不选
				dqclick("xn",".xnclass");
				//点击西北地区全选与全不选
				dqclick("xb",".xbclass");
				//点击其他地区全选与全不选
				dqclick("qt",".qtclass");

				/*单个地区选中效果*/
				$(".all-area dd").click(function(){
					var id=$(this).attr("id");
					if(id!="qg"&&id!="hb"&&id!="db"&&id!="hd"&&id!="zn"&&id!="xn"&&id!="xb"&&id!="qt"){
						var arrClass = $(this).attr("class");
						//没选中则选中
						if (!arrClass || arrClass.indexOf("active-area") < 0){
							$(this).addClass("active-area");
						//选中则不选中
						}else{
							$(this).removeClass("active-area");
						} 
					}
					//单个省份牵扯地区选中状态
					dqstatus("hb",".hbclass",".hb");
					dqstatus("db",".dbclass",".db");
					dqstatus("hd",".hdclass",".hd");
					dqstatus("zn",".znclass",".zn");
					dqstatus("xn",".xnclass",".xn");
					dqstatus("xb",".xbclass",".xb");
					dqstatus("qt",".qtclass",".qt");
					//地区选中与不选中牵扯全国选中状态
					qgstatus();
				});
				
				//区域选中效果
				function dqclick(id,Class){
					qdcheck(id,Class,qgstatus());
				}
				//选中效果
				function qdcheck(id,Class,func){
					$("#"+id).click(function(){
						var arrClass = $(this).attr("class");
						//之前没选中则全选
						if (!arrClass ||arrClass.indexOf("active-area") < 0){
							$(Class+" dd").each(function(){
								if($(this).attr("id")=="qg" || $(this).attr("name")=="dq"){
									$(this).addClass("active-area1");
								}else{
									$(this).addClass("active-area");
								}
							});
						//之前选中则全不选
						}else{
							$(Class+" dd").each(function(){
								if($(this).attr("id")=="qg" || $(this).attr("name")=="dq"){
									$(this).removeClass("active-area1");
								}else{
									$(this).removeClass("active-area");
								}
							});
						}
						func;
					});
				}
				//单个省份牵扯地区选中状态
				function dqstatus(id,Class1,Class2){
					//地区个数
					var size =$(Class1 +" dd").size();
					//选中个数
					var xzgs=0;
					
					var tiaojian=id+" active-area"

					$(Class2).each(
						function(){
							if($(this).attr("class")==tiaojian){
								xzgs++;

							}
						}
					);
					if(xzgs < size-1){
						$("#"+ id).removeClass("active-area1 active-area");
					}else{
						$("#"+ id).addClass("active-area1 active-area");
					}
				}
				//地区选中与不选中牵扯全国选中状态
				function qgstatus(){
					//华北
					var hbclass =$("#hb").attr("class");
					//东北
					var dbclass =$("#db").attr("class");
					//华东
					var hdclass =$("#hd").attr("class");
					//中南
					var znclass =$("#zn").attr("class");
					//西南
					var xnclass =$("#xn").attr("class");
					//西北
					var xbclass =$("#xb").attr("class");
					//其他
					var qtclass =$("#qt").attr("class");
			
					if (hbclass=="active-area1 active-area" && dbclass=="active-area1 active-area" && hdclass=="active-area1 active-area" && znclass=="active-area1 active-area" && xnclass=="active-area1 active-area" && xbclass=="active-area1 active-area" && qtclass=="active-area1 active-area"){
						$("#qg").addClass("active-area1");
					}else{
						$("#qg").removeClass("active-area1");
					}
				}
				//地区编辑时初始化
				function initdq(){

					//单个省份牵扯地区选中状态
					dqstatus("hb",".hbclass",".hb");
					dqstatus("db",".dbclass",".db");
					dqstatus("hd",".hdclass",".hd");
					dqstatus("zn",".znclass",".zn");
					dqstatus("xn",".xnclass",".xn");
					dqstatus("xb",".xbclass",".xb");
					dqstatus("qt",".qtclass",".qt");
					//地区选中与不选中牵扯全国选中状态
					qgstatus();
				}
	    		
	    		
