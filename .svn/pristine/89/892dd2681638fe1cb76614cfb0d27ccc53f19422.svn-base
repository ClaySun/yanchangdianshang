		<script>
			$(function() {
				//拒绝
				$(".toDisplay").click(function() {
					$("#tr").show();
					$("#divbutton").show();
					$(".divsubmit").hide();
				});
				//提交
				$(".doSubmit").each(function() {
					$(this).click(function() {
						var v = $(this).attr("v");
						if (v == '') {
							gAlert("操作异常");
						}
						if (v == 2) {
							if ($("#remark").val() == '') {
								gAlert("请填写拒绝理由");
								return
							}
							if ($("#remark").val().length > 150) {
								gAlert("您填写的拒绝理由过长");
								return
							}
							$("#gform").attr("action", "/arplatform/exp/contract/sell/manage/return/toConfirm.do");
							$("#status").val('2');
							$("#gform").submit();
						} else {
							$("#gform").attr("action", "/arplatform/exp/contract/sell/manage/return/toConfirm.do");
							$("#status").val('3');
							$("#gform").submit();
						}
					});
				});
				//返回
				$(".doBack").click(function() {
					$("#gform").attr("action", "/arplatform/exp/contract/sell/manage/return/list.do");
					$("#gform").submit();
				});
				if ('1' == 1 || '1' == '1') {
					$("#tr").hide();
				}
			});
		</script>
