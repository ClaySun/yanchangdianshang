			}
			div.zoomDiv {
				z-index: 999;
				position: absolute;
				top: 0px;
				right: 0px;
				width: 400px !important;
				height: 360px !important;
				background: #ffffff;
				border: 1px solid #CCCCCC;
				display: none;
				text-align: center;
				overflow: hidden;
				margin-left: 20px;
			}
			div.zoomMask {
				position: absolute;
				background: url(./images/mask.png) repeat scroll 0 0 transparent;
				cursor: move;
				z-index: 1;
			}
			/*放大镜*/
			.u-flyer {
				display: block;
				width: 50px;
				height: 50px;
				border-radius: 50px;
				position: fixed;
				z-index: 9999;
			}
		</style>
		<script>
			$(document).ready(function() {
				var imgul = '<a href="javascript:void(0)"><img src="images/goods02.jpg" rel="images/goods02.jpg" class="jqzoom details-img-big" style=" position: relative;" /></a>' + '<ul class="tb-thumb" id="thumblist">' + '	<li class="tb-selected"><div class="tb-pic tb-s40"><a href="javascript:void(0)"><img src="images/goods02.jpg" mid="images/goods02.jpg" big="images/goods02.jpg"></a></div></li>' + '	<li><div class="tb-pic tb-s40"><a href="javascript:void(0)"><img src="images/goods03.jpg" mid="images/goods03.jpg" big="images/goods03.jpg"></a></div></li>' + '	<li><div class="tb-pic tb-s40"><a href="javascript:void(0)"><img src="images/goods02.jpg" mid="images/goods02.jpg" big="images/goods02.jpg"></a></div></li>' + '	<li><div class="tb-pic tb-s40"><a href="javascript:void(0)"><img src="images/goods03.jpg" mid="images/goods03.jpg" big="images/goods03.jpg"></a></div></li>' + '	<li><div class="tb-pic tb-s40"><a href="javascript:void(0)"><img src="images/goods02.jpg" mid="images/goods02.jpg" big="images/goods02.jpg"></a></div></li>' + '</ul>';
				$(".display-area").append(imgul);
				$(".jqzoom").imagezoom();
				$("#thumblist li a").live("click", function() {
					$(this).parents("li").addClass("tb-selected").siblings().removeClass("tb-selected");
					$(".jqzoom").attr('src', $(this).find("img").attr("mid"));
					$(".jqzoom").attr('rel', $(this).find("img").attr("big"));
				});
				$("#d").Spinner();
				var offset = $("#end").offset();
				$(".addcar").click(function(event) {
					var addcar = $(this);
					var img = $(".display-area").find('img:first').attr('src');
					var flyer = $('<img class="u-flyer" src="' + img + '">');
					flyer.fly({
						start : {
							left : event.pageX,
							top : event.pageY
						},
						end : {
							left : offset.left + 10,
							top : offset.top + 10,
							width : 0,
							height : 0
						},
						onEnd : function() {
							$("#msg").show().animate({
								width : '250px'
							}, 200).fadeOut(1000);
							addcar.css("cursor", "default").removeClass('orange').unbind('click');
							this.destory();
						}
					});
					var carnum = $("#rcart").text();
					carnum++;
					$("#rcart").text(carnum)
				});
			});
		</script>
