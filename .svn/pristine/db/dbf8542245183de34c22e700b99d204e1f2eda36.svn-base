			.tb-booth {
				border: 1px solid #CDCDCD;
				position: relative;
				z-index: 1;
			}
			.tb-thumb .tb-selected {
				background: none repeat scroll 0 0 #bdbdbd;
				height: 58px;
				padding: 2px;
			}
			.tb-thumb .tb-selected div {
