/***中间轮播广告***/

.banner {
	min-width: 1190px;
	height: 400px;
	overflow: hidden;
	position: relative;
	margin: 0 auto;
	z-index: 1;
	background:#f0f0f0;
}

.banner .hd {
	top: auto;
	right: auto;
	bottom: auto;
	left: auto;
	position: absolute;
	width: 100%;
	bottom: 10px;
	text-align: center;
	font-size: 0;
	z-index: 1
}

.banner .hd li {
	*display: inline;
	zoom: 1;
	width: 10px;
	height: 10px;
	border-radius: 50%;
	overflow: hidden;
	margin: 0 5px;
	background-color: #FFF;
	opacity: 0.8;
	cursor: pointer
}

.banner .hd .on {
	background-color: #e60012;
}

.banner .bd {
	position: relative;
	height: 100%;
	z-index: 0;
	overflow: hidden
}

.banner .bd li {
	zoom: 1;
	width: 100%;
	height: 400px;
	overflow: hidden;
	vertical-align: middle;
	text-align: center
}

.banner .bd li a {
	width: 1190px;
	height: 400px;
	margin: 0 auto;
	display: inline-block
}

.banner .bd li img {
	display: inline-block;
	float: left;
	vertical-align: middle;
	margin-left: 224px
}

.banner .bd img {
	display: block
}

.banner .prev,
.banner .next {
	display: none;
	top: 50%;
	right: auto;
	bottom: auto;
	left: auto;
	position: absolute;
	z-index: 10;
	width: 50px;
	height: 70px;
	border-radius: 3px;
	margin: -60px 0 0;
	background: url("../images/ad_ctr.png") no-repeat 0 0;
	cursor: pointer;
	text-indent: -9999px;
	opacity: 0.2
}

.banner .prev {
	left: 5px;
	background-position: 0 -10px
}

.banner .next {
	right: 40px;
	background-position: -10px -100px
}

.banner .prev:hover,
.banner .next:hover {
	opacity: 0.5
}

