				$(".goods-modify").click(function() {
					layer.open({
						type : 1,
						title : '<h2 style=" width:100%; height: 40px;line-height: 40px;font-size: 18px;font-weight: bold; text-align:center; color: #fff;background-color: #f00;">' + '修改库存' + '</h2>',
