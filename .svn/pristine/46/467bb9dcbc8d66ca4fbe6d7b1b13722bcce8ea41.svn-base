					});
				})
				$("#vip-cancel").live("click", function() {
					layer.closeAll()
				})
				$("#vip-ok").live("click", function() {
					layer.closeAll()
				})
				$(".layui-layer-close").live("click", function() {
					layer.closeAll()
				})
			})
